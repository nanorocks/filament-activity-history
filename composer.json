{
    "name": "nanorocks/filament-activity-history",
    "description": "Filament plugin for user activity",
    "keywords": [
        "nanorocks",
        "laravel",
        "filament-activity-history"
    ],
    "homepage": "https://github.com/nanorocks/filament-activity-history",
    "support": {
        "issues": "https://github.com/nanorocks/filament-activity-history/issues",
        "source": "https://github.com/nanorocks/filament-activity-history"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andrej Nankov",
            "email": "andrejnankov@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Nanorocks\\FilamentActivityHistory\\": "src/",
            "Nanorocks\\FilamentActivityHistory\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nanorocks\\FilamentActivityHistory\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nanorocks\\FilamentActivityHistory\\FilamentActivityHistoryServiceProvider"
            ],
            "aliases": {
                "FilamentActivityHistory": "Nanorocks\\FilamentActivityHistory\\Facades\\FilamentActivityHistory"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
